%!PS-Adobe
.7 setgray
300 400 translate
/polygono {
4 dict begin 
/N exch def /r exch def 
/A 360 N div def
r 0 moveto
N { A cos r mul A sin r mul lineto /A A 360 N div add def} repeat closepath
end } def
/klxy 0 def 
200 /klxy exch store
/klxx 0 def 
45 /klxx exch store
gsave
1.300000 1.300000 scale 50 neg 
150 neg 
translate
0.100000 0 0.400000  setrgbcolor
90 klxx add rotate newpath 0 0 60 30 330 arc
0 0 lineto
fill
grestore
gsave
0 1 0 setrgbcolor
newpath 0 0 60 30 330 arc
0 0 lineto
fill
grestore
/klxb 0 def 
0.700000 /klxb exch store
9  { 4 dict begin
gsave
klxb klxb scale 50 50 neg 
translate
0.800000 0 0.700000  setrgbcolor
newpath 0 0 60 0 360 arc
0 0 lineto
fill
grestore
klxb 0.050000 add /klxb exch store
end
 } repeat
/klxz 0 def 
0.500000 /klxz exch store
{
klxx 0 gt klxy 0 gt and  not {exit} if
4 dict begin
gsave
klxz 0.800000 0.500000  setrgbcolor
klxx klxy translate
60 3 polygono
fill
grestore
klxx 1 sub /klxx exch store
klxy 1 sub /klxy exch store
end
} loop
gsave
newpath 0 0 60 0 360 arc
0 0 lineto
fill
grestore

showpage
